{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/emzmono/src",
  "monorepo": true,
  "root": "apps/emzmono",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/emzmono/tsconfig.app.json"
  },
  "projects": {
    "emzmono": {
      "type": "application",
      "root": "apps/emzmono",
      "entryFile": "main",
      "sourceRoot": "apps/emzmono/src",
      "compilerOptions": {
        "tsConfigPath": "apps/emzmono/tsconfig.app.json"
      }
    },
    "schedule": {
      "type": "application",
      "root": "apps/schedule",
      "entryFile": "main",
      "sourceRoot": "apps/schedule/src",
      "compilerOptions": {
        "tsConfigPath": "apps/schedule/tsconfig.app.json"
      }
    },
    "tenant-front": {
      "type": "application",
      "root": "apps/tenant-front",
      "entryFile": "main",
      "sourceRoot": "apps/tenant-front/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tenant-front/tsconfig.app.json"
      }
    },
    "tenant-api": {
      "type": "application",
      "root": "apps/tenant-api",
      "entryFile": "main",
      "sourceRoot": "apps/tenant-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tenant-api/tsconfig.app.json",
        "assets": ["_proto/**/*.proto"],
        "watchAssets": true
      }
    },
    "grpc": {
      "type": "application",
      "root": "apps/grpc",
      "entryFile": "main",
      "sourceRoot": "apps/grpc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/grpc/tsconfig.app.json",
        "assets": ["_proto/**/*.proto"],
        "watchAssets": true
      }
    }
  }
}
