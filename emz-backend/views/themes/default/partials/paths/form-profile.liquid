<div class='row g-0 emz-form'>
    <div class='col-12'>
        <h3 class='widget-profile__label'>
            Thông tin nhận hàng
            <a href='#' class='blue'>
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-person-circle" viewBox="0 0 16 16">
                    <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z"/>
                    <path fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z"/>
                </svg>
                Đăng nhập
            </a>
        </h3>
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for="inputProfileControl1" class="form-label">Họ tên </label>{% endcomment %}
        <input type="text" class="form-control" id="inputProfileControl1" placeholder="Họ tên ">
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for="inputProfileControl2" class="form-label">Số điện thoại<span class='required'>*</span></label>{% endcomment %}
        <input type="text" class="form-control" id="inputProfileControl2" placeholder="Số điện thoại">
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for="inputProfileControl3" class="form-label">Địa chỉ<span class='required'>*</span></label>{% endcomment %}
        <input type="text" class="form-control" id="inputProfileControl3" placeholder="Địa chỉ">
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for='inputProfileControl4' class="form-label">Tỉnh / Thành phố<span class='required'>*</span></label>{% endcomment %}
        <select id='inputProfileControl4' class="form-select" aria-label="Default select example">
            <option selected>Tỉnh thành</option>
            <option value="1">One</option>
            <option value="2">Two</option>
            <option value="3">Three</option>
        </select>
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for='inputProfileControl5' class="form-label">Quận huyện<span class='required'>*</span></label>{% endcomment %}
        <select id='inputProfileControl5' class="form-select" aria-label="Default select example">
            <option selected>Quận huyện</option>
            <option value="1">One</option>
            <option value="2">Two</option>
            <option value="3">Three</option>
        </select>
    </div>
    <div class='col-12 mb-3'>
        <div class="form-floating">
            <textarea class="form-control" placeholder="Lưu ý cho người bán…" id="inputProfileControl6"></textarea>
        </div>
    </div>
</div>
