<div class='row g-0 emz-form'>
    <div class='col-12 mb-3'>
        <label for="inputProfileEditControl1" class="form-label">Tên của bạn</label>
        <input type="text" class="form-control" id="inputProfileEditControl1" placeholder="Lý Linh Ngọc">
    </div>
    <div class='col-12 mb-3'>
        <label for="inputProfileEditControl2" class="form-label">Số điện thoại<span class='required'>*</span></label>
        <input type="text" class="form-control" id="inputProfileEditControl2" placeholder="0983 453 005">
    </div>
    <div class='col-12 mb-3'>
        <label for="inputProfileEditControl3" class="form-label">Email</label>
        <input type="text" class="form-control" id="inputProfileEditControl3" placeholder="ngoc@gmail.com">
    </div>
    <div class='col-12 mb-3'>
        <input class="form-check-input emz-form rounded-pill mt-0" type="checkbox" value="" id="inputProfileEditControl4" checked>
        <label for='inputProfileEditControl4' class='ms-2'>Đổi mật khẩu</label>
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for="inputProfileEditControl5" class="form-label">Mật khẩu cũ</label>{% endcomment %}
        <input type="text" class="form-control" id="inputProfileEditControl5" placeholder="Mật khẩu cũ">
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for="inputProfileEditControl6" class="form-label">Mật khẩu mới</label>{% endcomment %}
        <input type="text" class="form-control" id="inputProfileEditControl6" placeholder="Nhập mật khẩu mới">
    </div>
    <div class='col-12 mb-3'>
        {% comment %}<label for="inputProfileEditControl7" class="form-label">Nhập lại mật khẩu mới</label>{% endcomment %}
        <input type="text" class="form-control" id="inputProfileEditControl7" placeholder="Nhập lại mật khẩu mới">
    </div>
</div>
