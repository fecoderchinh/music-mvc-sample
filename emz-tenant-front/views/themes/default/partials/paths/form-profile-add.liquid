<div class='row g-0 emz-form'>
    <div class='col-12'>
        <div class='row g-0 two-cols-20px-spacing'>
            <div class='col-lg-6 col-md-12 mb-3'>
                <label for="inputProfileAddControl1" class="form-label">Họ và tên<span class='required'>*</span></label>
                <input type="text" class="form-control" id="inputProfileAddControl1" placeholder="Nhập họ và tên">
            </div>
            <div class='col-lg-6 col-md-12 mb-3'>
                <label for="inputProfileAddControl2" class="form-label">Số điện thoại<span class='required'>*</span></label>
                <input type="text" class="form-control" id="inputProfileAddControl2" placeholder="Nhập số điện thoại">
            </div>
        </div>
    </div>
    <div class='col-12 mb-3'>
        <label for="inputProfileAddControl3" class="form-label">Địa chỉ<span class='required'>*</span></label>
        <input type="text" class="form-control" id="inputProfileAddControl3" placeholder="Nhập địa chỉ">
    </div>
    <div class='col-12'>
        <div class='row g-0 two-cols-20px-spacing'>
            <div class='col-lg-6 col-md-12 mb-3'>
                <label class="form-label">Tỉnh / Thành phố<span class='required'>*</span></label>
                <select class="form-select" aria-label="Default select example">
                    <option selected>Chọn tỉnh thành</option>
                    <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option>
                </select>
            </div>
            <div class='col-lg-6 col-md-12 mb-3'>
                <label class="form-label">Quận huyện<span class='required'>*</span></label>
                <select class="form-select" aria-label="Default select example">
                    <option selected>Chọn quận huyện</option>
                    <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option>
                </select>
            </div>
        </div>
    </div>
    <div class='col-12'>
        <div class='row g-0 two-cols-20px-spacing'>
            <div class='col-lg-6 col-md-12 mb-3'>
                <label class="form-label">Phường xã<span class='required'>*</span></label>
                <select class="form-select" aria-label="Default select example">
                    <option selected>Chọn phường xã</option>
                    <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option>
                </select>
            </div>
            <div class='col-lg-6 col-md-12 mb-3'>
                <label for="inputProfileAddControl4" class="form-label">Ghi chú cho địa chỉ này</label>
                <input type="text" class="form-control" id="inputProfileAddControl4" placeholder="">
            </div>
        </div>
    </div>
</div>
